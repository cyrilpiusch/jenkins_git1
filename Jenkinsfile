pipeline{
  agent any
      stages{
        stage('Build'){
          steps{
            echo 'Starts to build'
          }
        }

        stage('Prepare'){
          steps{
            echo 'prepare for test'
          }
        }
        stage('Test'){
          steps{
            echo 'testing'
          }
        }

        stage('Deploy'){
          steps{
            echo 'ready to deploy'
          }
        }
      }
}
